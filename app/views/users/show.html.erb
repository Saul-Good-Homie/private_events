<h1><%= @user.username %></h1>


<h2><%= "Created Events" %></h2>
<% @user.created_events.each do |event| %>
    <tr>
      <td><%= event.description %></td></br>
      <td><%= event.location %></td></br>
      <td><%= event.date %></td></br>
    </tr>
    </br>
  <% end %>


<%= link_to 'Edit', edit_user_path(@user) %> |
